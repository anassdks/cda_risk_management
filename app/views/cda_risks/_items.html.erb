<tr class="risk-matrix__cell-item"><td><%= item.id %></td><td><%= link_to item.name, item %></td><td><%= given_date == '' ? item.severity_value.to_i : item.severity_value_given_date.to_i  %></td><td><%= item.custom_values.detect { |i| i.custom_field_id == Setting.plugin_cda_risk_management['plan_action_cf_id'].to_i}.try(:value) == '1' ? 'oui' : 'non'  %></td><td><%= item.custom_values.detect { |i| i.custom_field_id == Setting.plugin_cda_risk_management['plan_action_cf_id'].to_i}.try(:value) == '1' ? item.custom_values.detect { |i| i.custom_field_id == Setting.plugin_cda_risk_management['probabilite_apres_pac_cf_id'].to_i}.try(:value).delete("^0-9-.").to_i * item.custom_values.detect { |i| i.custom_field_id == Setting.plugin_cda_risk_management['criticite_apres_pac_cf_id'].to_i}.try(:value).delete("^0-9-.").to_i : ''  %></td></tr>
